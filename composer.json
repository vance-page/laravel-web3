{
    "name": "vance-page/laravel-web3",
    "description": "Laravel SDK wrapper for the Web3 PHP API client that interacts with the Ethereum blockchain.",
    "keywords": ["laravel", "php", "api", "sdk", "wrapper", "web3", "eth", "ethereum", "blockchain", "token", "contracts"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/vance-page/laravel-web3",
    "authors": [
        {
            "name": "vance",
            "homepage": "https://github.com/vance-page",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "vance-page/web3.php": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vance\\LaravelWeb3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vance\\LaravelWeb3\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Vance\\LaravelWeb3\\LaravelWeb3ServiceProvider"
            ]
        }
    }
}
